{
  "manifest_version": 2,
  "name": "Aloha",
  "version": "1.0",
  "description": "Aloha replaces your boring looking new tabs with beautiful Photos and Quotes.",
  "author":"Rishabh Choudhary",
  "icons":{
    "48": "icons/icon.png",
    "98": "icons/bigIcon.png",
    "128": "icons/appIcon.png"
  },
  "browser_action":{
    "default_icon": "icons/appIcon.png",
    "default_title": "Aloha"
  },
  "chrome_url_overrides":{
    "newtab": "dashboard.html"
    },
  "chrome_settings_overrides":{
    "homepage": "dashboard.html"
  },
  "background":{
    "scripts": ["scripts/background.js"]
  },
  "permissions":["tabs","storage"]
}
